{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HackerNewsApi": {
    "baseUrl": "https://hacker-news.firebaseio.com/v0",
    "itemPath": "item",
    "storiesPath": "topstories"
  },
  "security": {
    "static": {
      "systems": [
        {
          "identifier": "",
          "key": "",
          "role": "",
          "apiUser": "",
          "apiSecret": ""
        }
      ]
    },
    "jwt": {
      "nonceValidation": false,
      "allowClockSkewSeconds": 0
    },
    "useStaticUsers": true
  }

}
